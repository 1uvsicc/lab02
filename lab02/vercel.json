{
    "version": 2,
    "builds": [
      {
        "framework": "nextjs",
        "src": "package.json",
        "use": "@vercel/node",
        "rewrites": [
            {"source": "/(.*)", "destination": "/"}
        ],
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "dist"
        }
        
      }
    ]
  }