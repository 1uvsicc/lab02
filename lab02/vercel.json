{
    "version": 2,
     "rewrites": [
            {"source": "/(.*)", "destination": "/"}
        ],
    "builds": [
      {
       
        "src": "package.json",
        "use": "@vercel/node",
       
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "dist"
        }
        
      }
    ],
    "routes": [ { "src": "/(.*)", "dest": "/index.html" }]
  }